{
  "name": "FableProject",
  "version": "1.0.0",
  "description": "FableProject",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "browser-sync": "^2.26.3",
    "concurrently": "^4.0.1",
    "fable-splitter": "^2.0.2"
  },
  "scripts": {
    "postinstall": "dotnet restore",
    "build": "fable-splitter src\\FableProject.fsproj --outDir output",
    "watch": "npm run build -- --watch",
    "sync":"browser-sync start -s output --extensions js -w --files *.*",
    "start": "concurrently \"npm run watch\" \"npm run sync\""
  }
}
