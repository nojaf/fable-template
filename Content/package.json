{
  "name": "FableProject",
  "version": "1.0.0",
  "description": "FableProject",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.1.5",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "concurrently": "4.0.1",
    "fable-compiler-dotnet": "2.1.0-beta-002",
    "fable-splitter": "2.1.0-beta-002",
    "jest": "23.6.0",
    "parcel": "1.10.3",
    "rimraf": "2.6.2"
  },
  "scripts": {
    "postinstall": "dotnet restore",
    "clean": "rimraf \"{output/*.!(html|ico),output/*/,dist/}\"",
    "compile": "fable-splitter src\\FableProject.fsproj --config splitter.config.js --allFiles",
    "compile:watch": "npm run compile -- --watch",
    "prebundle": "npm run compile",
    "bundle": "parcel build output/index.html",
    "bundle:watch": "parcel serve output/index.html",
    "prestart": "npm run compile",
    "start": "concurrently \"npm run compile:watch\" \"npm run bundle:watch\" \"npm run test:watch\"",
    "pretest": "npm run compile",
    "test": "jest",
    "test:watch": "jest --watchAll"
  }
}
